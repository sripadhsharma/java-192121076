import java.util.Scanner;
public class main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the first denomination: ");
        long num=sc.nextInt();
		System.out.println("Enter the first denomination no of notes: ");
		long num1=sc.nextInt();
		System.out.println("Enter the second denomination: ");
	    long num2=sc.nextInt();
		System.out.println("Enter the second denomination no of notes: ");
		long num3=sc.nextInt();
		System.out.println("Enter the third denomination: ");
		long num4=sc.nextInt();
		System.out.println("Enter the third denomination no of notes: ");
		long num5=sc.nextInt();
		System.out.println("Enter the fourth denomination: ");
		long num6=sc.nextInt();
		System.out.println("Enter the fourth denomination no of notes: ");
		long num7=sc.nextInt();
		long balance=((num*num1)+(num2*num3)+(num4*num5)+(num6*num7));
		System.out.println("Total Available Balance in ATM: "+balance);
		}
}
